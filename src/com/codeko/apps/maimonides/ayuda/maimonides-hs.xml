<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE helpset PUBLIC "-//Sun Microsystems Inc.//DTD JavaHelp HelpSet Version 2.0//EN" "http://java.sun.com/products/javahelp/helpset_2_0.dtd">
<helpset version="2.0">
    <title>Maimónides: Manual de usuario</title>
    <maps>
        <homeID>maimonides.about</homeID>
        <mapref location="maimonides-map.xml"/>
    </maps>
    <view mergetype="javax.help.AppendMerge">
        <name>TOC</name>
        <label>Tabla de Contenidos</label>
        <type>javax.help.TOCView</type>
        <data>maimonides-toc.xml</data>
        <image>contentIcon</image>
    </view>
    <view mergetype="javax.help.AppendMerge">
        <name>Index</name>
        <label>Índice</label>
        <type>javax.help.IndexView</type>
        <data>maimonides-idx.xml</data>
        <image>homeIcon</image>
    </view>
    <view>
        <name>Buscar</name>
        <label>Buscar</label>
        <type>javax.help.SearchView</type>
        <data>JavaHelpSearch</data>
        <image>searchIcon</image>
    </view>
    <view>
        <name>Favoritos</name>
        <label>Favoritos</label>
        <type>javax.help.FavoritesView</type>
        <image>bookmarkIcon</image>
    </view>
    <presentation default="true">
        <name>principal</name>
        <title>Maimónides: Manual de usuario</title>
        <image>iconoPrincipal</image>
        <location x="200" y="200" />
        <toolbar>
            <helpaction image="backIcon">javax.help.BackAction
            </helpaction>
            <helpaction image="forwardIcon">javax.help.ForwardAction
            </helpaction>
            <helpaction image="homeIcon">javax.help.HomeAction
            </helpaction>
            <helpaction image="reloadIcon">javax.help.ReloadAction
            </helpaction>
            <helpaction image="addBookmarkIcon">javax.help.FavoritesAction
            </helpaction>
            <helpaction image="printIcon">javax.help.PrintAction
            </helpaction>
            <helpaction image="printSetupIcon">javax.help.PrintSetupAction
            </helpaction>
        </toolbar>
    </presentation>
</helpset>
